&nbsp;
<%- if controller_name != 'sessions' %>
  <%= link_to t('btn.sign_in'), new_session_path(resource_name) %>
<% end -%>
&nbsp;
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t('btn.sign_up'), new_registration_path(resource_name)  %>
<% end -%>
&nbsp;
<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to t('btn.forgot_password'), new_password_path(resource_name)  %>
<% end -%>
